{
  "name": "br-ext",
  "displayName": "Mouse Chat",
  "description": "Your very own mouse assistantüê≠.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "configuration": {
      "title": "Ollama",
      "properties": {
        "ollama.apiUrl": {
          "type": "string",
          "default": "http://localhost:11434",
          "description": "URL of the Ollama API"
        }
      }
    },
    "commands": [
      {
        "command": "vscode-ollama.run",
        "title": "Mouse Chat"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test",
    "docker:build": "docker-compose build",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "docker:ps": "docker-compose ps",
    "docker:restart": "docker-compose restart",
    "docker:start": "npm run docker:build && npm run docker:up",
    "docker:stop": "npm run docker:down",
    "docker:clean": "docker-compose down -v --rmi all --remove-orphans"
  },
  "dependencies": {
    "vscode": "*"
  },
  "devDependencies": {
    "@types/node": "^9.4.6",
    "typescript": "^2.8.3"
  }
}